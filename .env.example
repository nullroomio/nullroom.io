# Kamal Deployment Environment Variables
# Copy this file to your shell profile (~/.bashrc or ~/.zshrc)
# or load it before running kamal commands: source .env

# GitHub Container Registry password (Personal Access Token)
# Create at: https://github.com/settings/tokens
# Required scopes: read:packages, write:packages, delete:packages
export KAMAL_REGISTRY_PASSWORD=ghp_your_token_here

# Production server IP address
export KAMAL_WEB_IP=your.server.ip.address
